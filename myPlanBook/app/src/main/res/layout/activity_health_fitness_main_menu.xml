<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/healthFitnessConstraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B4FC5B"
    tools:context=".HealthFitnessMainMenu.HealthFitnessMainMenuActivity">

    <TextView
        android:id="@+id/textViewHealthFitnessTitle"
        android:layout_width="match_parent"
        android:layout_height="70sp"
        android:background="@drawable/square_outline_bordered"
        android:gravity="center"
        android:text="HEALTH AND FITNESS"
        android:textColor="#000000"
        android:textSize="35sp"
        app:fontFamily="sans-serif-thin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:popupTheme="@style/AppTheme.PopupOverlay" />

    <include
        android:id="@+id/include"
        layout="@layout/content_health_fitness_main_menu"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textViewHealthFitnessTitle"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>